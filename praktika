import os
from lxml import etree
parser=etree.XMLParser(remove_comments=True)
def lib(xmlFile):
    lib={}
    lib[xmlFile.getroot()]=lib_chil(xmlFile.getroot())  
    return lib

def lib_chil(chil):
    lib={}
    if chil.getchildren() != None :
        for child_root in chil:
            lib_elem=lib_chil(child_root)
            lib[child_root.tag]=lib_elem
    else:
        return chil.text
    return lib
fails=[]
fails=os.listdir("training")
for i in fails:
    with open("training/"+i) as fobj:
        xml = etree.parse(fobj,parser)
    with open('wraite','w') as wraite:
        string=str(lib(xml))
        wraite.write(string)#вызов чтения файла
        wraite.write('1111111111111111111111')
